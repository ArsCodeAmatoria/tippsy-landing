{
  "images": {
    "sizes": [640, 750, 828, 1080, 1200, 1920],
    "domains": ["tippsy.me", "tippsy-landing-pub64asgf-kojin-foxs-projects.vercel.app"],
    "remotePatterns": [
      {
        "protocol": "https",
        "hostname": "**"
      }
    ]
  },
  "headers": [
    {
      "source": "/images/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ]
} 